{
  "log": {
    "disabled": true,
    "level": "info",
    "timestamp": true
  },
  "inbounds": [
    {
      "type": "hysteria2",
      "tag": "hy2-in",
      "up_mbps": 100,
      "down_mbps": 100,
      "obfs": {
        "type": "salamander",
        "password": "-"
      },
      "users": [
        {
          "name": "-",
          "password": "-"
        }
      ],
      "ignore_client_bandwidth": false,
      "tls": {
        "enabled": true,
        "server_name": "www.cloudflare.com",
        "certificate_path": "/etc/sing-box/hy2.crt",
        "key_path": "/etc/sing-box/hy2.key"
      },
      "listen": "::",
      "listen_port": 443,
      "sniff": true,
      "sniff_override_destination": true
    },
    {
      "type": "vless",
      "tag": "reality-in",
      "users": [
        {
          "name": "-",
          "uuid": "-",
          "flow": "xtls-rprx-vision"
        }
      ],
      "tls": {
        "enabled": true,
        "server_name": "www.cloudflare.com",
        "reality": {
          "enabled": true,
          "handshake": {
            "server": "www.cloudflare.com",
            "server_port": 443
          },
          "private_key": "-"
        }
      },
      "listen": "::",
      "listen_port": 8443,
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "domain_suffix": ["example.com"],
        "action": "reject"
      }
    ],
    "final": "direct"
  }
}
